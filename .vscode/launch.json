{
  "configurations": [
    {
      "type": "coreclr",
      "request": "launch",
      "name": "Launch .NET Console App",
      "program": "${workspaceFolder}/bin/Debug/net9.0/${input:executableName}.exe",
      "preLaunchTask": "build",
      "console": "integratedTerminal",
      "stopAtEntry": false
    }
  ],
  "inputs": [
    {
      "type": "promptString",
      "id": "executableName",
      "description": "Enter the name of your output executable (without .exe)"
    }
  ]
}